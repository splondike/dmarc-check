  build-depends:
   base >=4.7 && <4.8,
   HaskellNet-SSL == 0.3.0.0,
   HaskellNet >= 0.3,
   hsemail == 1.7.7,
   parsec >= 3.0,
   MissingH >= 1.0.0,
   zip-archive >= 0.2.3,
   bytestring >= 0.10.0,
   base64-bytestring >= 1.0.0.1,
   old-time >= 1.1.0.3,
   xml >= 1.3,
   conf >= 0.1.1.0,
   text >= 1.1.1.3

  -- hs-source-dirs:      
  default-language:    Haskell2010
